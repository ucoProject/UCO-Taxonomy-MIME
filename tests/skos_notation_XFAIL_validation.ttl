@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix uco-skos: <https://taxonomy.unifiedcyberontology.org/skos/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

[]
	a sh:ValidationReport ;
	sh:conforms "false"^^xsd:boolean ;
	sh:result
		[
			a sh:ValidationResult ;
			sh:focusNode <http://example.org/kb/concept-1> ;
			sh:resultMessage "SKOS Notations must be unique within concept schemes." ;
			sh:resultSeverity sh:Violation ;
			sh:sourceConstraint [
				a sh:SPARQLConstraint ;
				rdfs:seeAlso <https://www.w3.org/TR/skos-reference/#notations> ;
				sh:message "SKOS Notations must be unique within concept schemes."@en ;
				sh:prefixes <http://www.w3.org/2004/02/skos/core#> ;
				sh:select """
			SELECT $this $value
			WHERE {
				$this
					skos:notation $value ;
					skos:inScheme ?nScheme ;
					.
				?nOther
					skos:notation $value ;
					skos:inScheme ?nScheme ;
					.
				FILTER ( $this != ?nOther ) .
			}
			""" ;
			] ;
			sh:sourceConstraintComponent sh:SPARQLConstraintComponent ;
			sh:sourceShape uco-skos:notation-shape ;
			sh:value "Name 1" ;
		] ,
		[
			a sh:ValidationResult ;
			sh:focusNode <http://example.org/kb/concept-2> ;
			sh:resultMessage "SKOS Notations must be unique within concept schemes." ;
			sh:resultSeverity sh:Violation ;
			sh:sourceConstraint [
				a sh:SPARQLConstraint ;
				rdfs:seeAlso <https://www.w3.org/TR/skos-reference/#notations> ;
				sh:message "SKOS Notations must be unique within concept schemes."@en ;
				sh:prefixes <http://www.w3.org/2004/02/skos/core#> ;
				sh:select """
			SELECT $this $value
			WHERE {
				$this
					skos:notation $value ;
					skos:inScheme ?nScheme ;
					.
				?nOther
					skos:notation $value ;
					skos:inScheme ?nScheme ;
					.
				FILTER ( $this != ?nOther ) .
			}
			""" ;
			] ;
			sh:sourceConstraintComponent sh:SPARQLConstraintComponent ;
			sh:sourceShape uco-skos:notation-shape ;
			sh:value "Name 1" ;
		]
		;
	.

